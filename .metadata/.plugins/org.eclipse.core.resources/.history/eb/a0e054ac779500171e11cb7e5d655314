package com.lee.test;

import org.junit.Test;

public class JudgeCircle {
	@Test
	public void test(){
		
	}
	
	
	public boolean judgeCircle(String moves) {
	       int y = 0;
			int x = 0;
			char[] chats = moves.toCharArray()
			for (char c : chats) {
				
				
				switch (c) {
				case 'U':
					y++;
					break;

				default:
					break;
				}
				
				if (c.equals("U")) {
					
				}else if (c.equals("D")) {
					y--;
				}else if (c.equals("L")) {
					x--;
				}else if (c.equals("R")) {
					x++;
				}
			}
			return x==0 && y==0;
	    }
	
}
