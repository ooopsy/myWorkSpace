package com.lee.test;

public class IslandPerimeter {
	
    public int islandPerimeter(int[][] grid) {
    
    	int perimeter = 0;
    	
    	for (int i = 0; i < grid.length; i++) {
			for (int j = 0; j < grid[i].length; j++) {
				try {
						
					int top = i -1;
					int button = i +1;
					int left = i-1;
					int right = i+1;
					
					if(int[i-1][j] == 1){
						perimeter--;
					}
					
					if(int[i+1][j] == 1){
						perimeter--;
					}
					
					if(int[i][j-1] == 1){
						perimeter--;
					}
					
					if(int[i][j+1] == 1){
						perimeter--;
					}
					
					
				} catch (Exception e) {
					
				}
			}
		}
    	
    	
    	return 0;
    }
    
    
}
